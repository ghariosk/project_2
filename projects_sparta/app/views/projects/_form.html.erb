<%= form_for([@user, @project]) do |f| %>

<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :desc %>
<%= f.text_field :desc %>

<%= f.label :image %>
<%= f.text_field :image %>

<%= f.label :git %>
<%= f.text_field :git %>

<%= f.label :approved %>
<%= f.check_box :approved %>


<%= f.submit %>


<% end %>